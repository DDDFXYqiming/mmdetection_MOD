{
    "pipeline": {
        "input": [
            "img"
        ],
        "output": [
            "post_output"
        ],
        "tasks": [
            {
                "type": "Task",
                "module": "Transform",
                "name": "Preprocess",
                "input": [
                    "img"
                ],
                "output": [
                    "prep_output"
                ],
                "transforms": [
                    {
                        "type": "LoadImageFromFile",
                        "backend_args": null
                    },
                    {
                        "type": "Resize",
                        "keep_ratio": true,
                        "size": [
                            640,
                            640
                        ]
                    },
                    {
                        "type": "Pad",
                        "pad_to_square": true,
                        "pad_val": {
                            "img": [
                                114.0,
                                114.0,
                                114.0
                            ]
                        }
                    },
                    {
                        "type": "Normalize",
                        "to_rgb": false,
                        "mean": [
                            0,
                            0,
                            0
                        ],
                        "std": [
                            1,
                            1,
                            1
                        ]
                    },
                    {
                        "type": "Pad",
                        "size_divisor": 32
                    },
                    {
                        "type": "DefaultFormatBundle"
                    },
                    {
                        "type": "Collect",
                        "meta_keys": [
                            "flip_direction",
                            "img_path",
                            "img_id",
                            "ori_filename",
                            "img_norm_cfg",
                            "pad_shape",
                            "scale_factor",
                            "valid_ratio",
                            "pad_param",
                            "img_shape",
                            "ori_shape",
                            "filename",
                            "flip"
                        ],
                        "keys": [
                            "img"
                        ]
                    }
                ]
            },
            {
                "name": "yolox",
                "type": "Task",
                "module": "Net",
                "is_batched": true,
                "input": [
                    "prep_output"
                ],
                "output": [
                    "infer_output"
                ],
                "input_map": {
                    "img": "input"
                },
                "output_map": {}
            },
            {
                "type": "Task",
                "module": "mmdet",
                "name": "postprocess",
                "component": "ResizeBBox",
                "params": {
                    "score_thr": 0.01,
                    "nms": {
                        "type": "nms",
                        "iou_threshold": 0.7
                    }
                },
                "output": [
                    "post_output"
                ],
                "input": [
                    "prep_output",
                    "infer_output"
                ]
            }
        ]
    }
}